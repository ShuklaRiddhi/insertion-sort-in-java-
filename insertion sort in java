public class main {
public static void main(string[] args){
int[] arr = {0 , -23,  56 , 18};
insertion(arr);
system.out.println("arrays.tostring(arr));

public void insertion(int[] arr) {
for (int i = 0; i < arr.length-1; i++) {
for (int j = i+1; j > 0; j--) {
if (arr[j] < arr[j-1])  {
swap(arr,j,second:j-1);
}
} else {
break;
}
}
}
} 




